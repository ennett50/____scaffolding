html, body
	margin 0
	padding 0
	border 0
	font inherit
	vertical-align baseline

article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section
		display block

blockquote, q
	quotes none

blockquote:before, blockquote:after,
q:before, q:after
	content none

table
	border-collapse collapse
	border-spacing 0

*:focus
	outline 0

ins
	text-decoration none

nav
	ul,li
		padding 0
		margin 0
		list-style none


input,
button,
textarea
	padding 0
	margin 0
	outline 0
	border 0
	display inline-block
	background transparent
	border-box()

textarea
	resize vertical
	
*
	font inherit
	color inherit